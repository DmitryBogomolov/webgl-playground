{
  "name": "webgl-playground",
  "version": "0.0.1",
  "description": "WebGL playground",
  "private": true,
  "scripts": {
    "check": "tsc -p tsconfig.check.json",
    "dev": "ts-node --project ./server/tsconfig.json ./server/index.ts",
    "dev2": "NODE_OPTIONS='--require ts-node/register' TS_NODE_PROJECT=tsconfig.devserver.json webpack serve",
    "build:server": "tsc --project ./server",
    "run:server": "node ./dist/server/index.js",
    "lint": "eslint --ext .ts .",
    "lint:fix": "npm run lint --fix",
    "test": "jest"
  },
  "keywords": [
    "webgl"
  ],
  "author": "Dmitry Bogomolov",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/mustache": "^4.1.1",
    "@types/node": "^14.14.37",
    "@types/webpack-dev-middleware": "^4.1.2",
    "@types/webpack-dev-server": "^3.11.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "jest": "^26.4.2",
    "mustache": "^3.2.1",
    "raw-loader": "^3.1.0",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.34.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
